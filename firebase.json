{
  "database": {
    "rules": "database.rules.json"
  },
  "hosting": {
    "public": "public/cinderella/dist",
    "rewrites": [
      {
        "source": "**",
        "destination": "index.html",
        "ignore": [
          "firebase.json",
          "**/.(css|js)",
          "**/node_modules/**"
        ]
      }
    ]
  }
}
